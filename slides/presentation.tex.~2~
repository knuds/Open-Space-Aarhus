\begin{frame}
  \frametitle{Aftenens program}
  \begin{itemize}
  \item Nyt fra OSAA, Hack Århus, Ubuntu \& seniorer uden grænser - \emph{hvad sker der i lokalerne?}
  \item 5 min pause - snacks
  \item Sven Pedersen: \emph{Bag om Hulivejen – En smartphone app.} (15 min)
  \item 5 min pause - snacks
  \item Alessio Civitillo: \emph{Using Open Source software in companies} (15 min)
  \item 30 min pause - snacks og networking
  \item Jens: \emph{Simulation og design af hydraulik systemer} (15 min)
  \item 5 min pause - snacks
  \item Kasper/Mark/Flemming: \emph{OSAAs TV-B-Gone / mini-POV klon} (15 min)
  \end{itemize}						
\end{frame}

\begin{frame}
  \frametitle{Nyt fra OSAA}
  \begin{itemize}
  \item OSAAs aktiviteter
    \begin{itemize}
    \item Forberedelse af workshops.
    \item FO Århus kurser (12 marts 2011 !)
    \end{itemize}
  \item Hvad vil du gerne have der skal ske!
  \item Følg med på \url{http://osaa.dk}
  \end{itemize}			
\end{frame}


\begin{frame}
  \frametitle{Nyt fra Hack Århus}
  \begin{itemize}
  \item Mødes hver tirsdag hygge, pizza og hacking:
  \item Afholdt mini workshops:
    \begin{itemize}
    \item Osciloscop
    \item Lab strømforsyning 
    \end{itemize}						

  \end{itemize}						
\end{frame}


