\begin{frame}
		\frametitle{Aftenens program}
		\begin{itemize}
			%\item Nyt fra OSAA - \emph{hvad sker der i lokalerne?}
			\item Christian Winther: \emph{Startup Weekend} (10 min)
			\item 5 min pause - snacks
		  \item Christian Perti: \emph{LASER (cutter og 3d printer) projektet} (30 min)
			\item 30 min pause - snacks og networking
			\item Flemming Frandsen: \emph{Udviklingsværktøj fra Dangerous Prototypes} (30 min)
			\item 5 min pause - snacks
			\item Jacob Toft Pedersen: \emph{Fractal compression, a technology in search of a problem} (15 min)
		\end{itemize}						
\end{frame}

