\begin{frame}
		\frametitle{Kommende aktiviteter}
		\begin{itemize}
				\item Zablet loddeworkshop d. 29/1 2011
				\item OSAA Julefrokost d. 29/1 2011 kl. 18

				\item Hack Århus: Tirsdage kl. 17-23	       	
				\item Ubuntu Århus: Torsdage i lige uger kl. 17-19 
				\item Python-gruppe: Følg med på \url{http://www.pygroups.dk/}
				\item OSAA bestyrelsesmøde: Torsdag d. 6/1 2011 kl. 19	 			
				\item Næste ${T^3}$: Tirsdag d. 1/2 2011 kl. 19.00
				\item Følg med på \url{http://osaa.dk}
		\end{itemize}						
\end{frame}

